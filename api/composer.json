{
  "type": "raiden",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "ext-intl": "*",
    "adhocore/jwt": "^1.1",
    "cesurapp/api-bundle": "^1.0",
    "cesurapp/media-bundle": "^1.0",
    "cesurapp/storage-bundle": "^1.0",
    "cesurapp/swoole-bundle": "^1.0",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^2.15",
    "symfony/console": "7.0.*",
    "symfony/dotenv": "7.0.*",
    "symfony/firebase-notifier": "7.0.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.0.*",
    "symfony/lock": "7.0.*",
    "symfony/mailer": "7.0.*",
    "symfony/mime": "7.0.*",
    "symfony/notifier": "7.0.*",
    "symfony/runtime": "7.0.*",
    "symfony/security-bundle": "7.0.*",
    "symfony/translation": "7.0.*",
    "symfony/twig-bundle": "7.0.*",
    "symfony/uid": "7.0.*",
    "symfony/validator": "7.0.*",
    "symfony/yaml": "7.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "7.0.*",
    "symfony/debug-bundle": "7.0.*",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/web-profiler-bundle": "7.0.*",
    "openswoole/ide-helper": "^22.1",
    "phpstan/phpstan": "^1.10",
    "php-cs-fixer/shim": "^3.41",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-symfony": "^1.3"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-intl-idn": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": "@auto-scripts",
    "post-update-cmd": "@auto-scripts",
    "qa:fix": "vendor/bin/php-cs-fixer fix --ansi --allow-risky=yes",
    "qa:lint": "vendor/bin/php-cs-fixer fix --dry-run --diff --ansi --allow-risky=yes",
    "qa:phpstan": "vendor/bin/phpstan analyse --no-progress --ansi --memory-limit 512m",
    "test": "bin/phpunit",
    "test:stop": "bin/phpunit --stop-on-failure --verbose --testdox --colors",
    "fix": [
      "@qa:fix",
      "@qa:phpstan",
      "@test"
    ]
  },
  "extra": {
    "symfony": {
      "require": "7.0.*"
    }
  }
}
