{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1.0",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-uuid": "*",
    "adhocore/jwt": "^1.1",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/orm": "^2.10",
    "dragonmantank/cron-expression": "^3.3",
    "odolbeau/phone-number-bundle": "^3.7",
    "symfony/console": "6.1.*",
    "symfony/dotenv": "6.1.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.1.*",
    "symfony/mime": "6.1.*",
    "symfony/runtime": "6.1.*",
    "symfony/security-bundle": "6.1.*",
    "symfony/translation": "6.1.*",
    "symfony/uid": "6.1.*",
    "symfony/validator": "6.1.*",
    "symfony/yaml": "6.1.*"
  },
  "require-dev": {
    "openswoole/ide-helper": "^4.10",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.1.*",
    "symfony/debug-bundle": "6.1.*",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/process": "6.1.*",
    "symfony/stopwatch": "6.1.*",
    "symfony/web-profiler-bundle": "6.1.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "exclude-from-classmap": [
      "src/**/Test/",
      "package/**/Test/"
    ],
    "psr-4": {
      "App\\": "src/",
      "Package\\": "package/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "src/**/Test/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-uuid": "*",
    "symfony/polyfill-intl-idn": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": "@auto-scripts",
    "post-update-cmd": "@auto-scripts",
    "analyse": "bin/tool/vendor/bin/phpstan analyse --no-progress --ansi --memory-limit 512m",
    "lint": "bin/tool/vendor/bin/php-cs-fixer fix --dry-run --diff --ansi --allow-risky=yes",
    "fix": "bin/tool/vendor/bin/php-cs-fixer fix --ansi --allow-risky=yes",
    "test": "bin/phpunit",
    "test-stop": "bin/phpunit --stop-on-failure",
    "test-coverage": "bin/phpunit --coverage-text",
    "install-tool": [
      "composer install --working-dir=bin/tool"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.1.*"
    }
  }
}
