{
  "name": "admin",
  "version": "0.0.1",
  "description": "Admin Application UI",
  "productName": "Admin App",
  "author": "Ramazan APAYDIN",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "test": "echo \"No test specified\" && exit 0",
    "fix": "yarn lint && yarn format",
    "dev": "quasar dev",
    "dev-ios": "quasar dev -m capacitor -T ios",
    "dev-android": "quasar dev -m capacitor -T android",
    "build": "quasar build",
    "build-ios": "quasar build -m capacitor -T ios --ide Xcode",
    "build-android": "quasar build -m capacitor -T android --ide 'Android Studio'"
  },
  "dependencies": {
    "@capacitor/core": "^5.0.0",
    "@capacitor/filesystem": "^5.0.0",
    "@capawesome-team/capacitor-file-opener": "^5.0.0",
    "@quasar/extras": "^1.15.11",
    "@vuelidate/validators": "^2.0.2",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.2.1",
    "countries-list": "^2.6.1",
    "dayjs": "^1.11.7",
    "firebase": "^9.9.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
